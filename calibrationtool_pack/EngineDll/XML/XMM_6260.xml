            <MaximumValidGainCorrectionValue>80</MaximumValidGainCorrectionValue>
            <RX3G_CW_OFFSET>1000000</RX3G_CW_OFFSET>
            <CommonGeneratorLevel>-60</CommonGeneratorLevel>
            <VerificationLimitHighGain>1.2</VerificationLimitHighGain>
