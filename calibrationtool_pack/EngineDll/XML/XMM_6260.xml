<?xml version="1.1" encoding="utf-8"?>
<Options XML_Version="1.6">
    <!-- Specifies name of base/platform xml file -->
    <Base>RF_Verification.xml</Base>
    <Project>XMM6260</Project>
    <Common>
        <RFChip>1</RFChip>
        <UseGTI4NVM>1</UseGTI4NVM>
        <SerialDTR>1</SerialDTR>
        <SerialRTS>0</SerialRTS>
        <Use_IPICOM>1</Use_IPICOM>
        <AutoScanInstruments>0</AutoScanInstruments>
        <ShowDialogMissingInstrument>0</ShowDialogMissingInstrument>
        <AutoTargetConnect>0</AutoTargetConnect>
        <ModuleTest>0</ModuleTest>
        <EnableCalPlots>1</EnableCalPlots>
        <UseBitmap>1</UseBitmap>
        <AddCmuDelay>0</AddCmuDelay>
        <Call_gcalucalkill>0</Call_gcalucalkill>
        <!-- Enables DWDIO/IPICOM serial communications and function call tracing -->
        <EnableDWDIOTrace>0</EnableDWDIOTrace>
        <RFVerification>1</RFVerification>
        <RFCalibration>1</RFCalibration>
        <InvalidateNVM>
		        <!-- 1 - ignore nvm mismatch,2 - abort on mismatch, 3 - invalidate nvm, 4 - always invalidate -->
		    <Action>2</Action>
        	<Group1>cal_rf_ue2</Group1>
        </InvalidateNVM>
        <RFProductionFlow>1</RFProductionFlow>
        <DualPortDiversity>1</DualPortDiversity>
        <TransitionAllowed>1</TransitionAllowed>
        <DisableLteVerif>1</DisableLteVerif>
        <!-- The delay of polling UTA mode status  -->
		<Ptestdelay>500</Ptestdelay>	
        <DoUcalKillBeforePmodeNormal>1</DoUcalKillBeforePmodeNormal>
        <DoGcalKillBeforePmodeNormal>1</DoGcalKillBeforePmodeNormal>	
        <RF_Connections>
        	<PortMappings>
        		<RFConnectionDescriptorList>
        			<RFConnectionDescriptor>
        				<AttTableName>Main</AttTableName>
        				<TargetPort>Main</TargetPort>
        				<TargetPortNumber>1</TargetPortNumber>
        				<TesterPort>A</TesterPort>
        			</RFConnectionDescriptor>
        			<RFConnectionDescriptor>
        				<AttTableName>Main</AttTableName>
        				<TargetPort>Diversity1</TargetPort>
        				<TargetPortNumber>2</TargetPortNumber>
        				<TesterPort>A</TesterPort>
        			</RFConnectionDescriptor>
        			<RFConnectionDescriptor>
        				<AttTableName>Main</AttTableName>
        				<TargetPortNumber>100</TargetPortNumber>
        				<TesterPort>B</TesterPort>
        			</RFConnectionDescriptor>					
        		</RFConnectionDescriptorList>
        	</PortMappings>
        </RF_Connections>
		<ForceSimpleCV>0</ForceSimpleCV>
    </Common>
    <Bridge>
        <Enable>0</Enable>
        <inputfilename>C:/Test_Reports/init_cfg_table_calib.inc</inputfilename>
        <outputfilename>C:/Test_Reports/output</outputfilename>
    </Bridge>
    <GSM>
        <Common>
            <!-- Production Verification enabling and disabling of measurements: 1-enable,0-disable -->
            <ProdVer_RXmeas>1</ProdVer_RXmeas>
            <ProdVer_TXmodmeas>1</ProdVer_TXmodmeas>
            <ProdVer_TXSwspecmeas>1</ProdVer_TXSwspecmeas>
            <ProdVer_TXModspecmeas>1</ProdVer_TXModspecmeas>
			<SimpleCV_MeasPVT>1</SimpleCV_MeasPVT>
			<SimpleCV_MeasSwitSpec>1</SimpleCV_MeasSwitSpec>
        </Common>
        <Rx>
            <PerformNewSUE2FastCal>1</PerformNewSUE2FastCal>
            <DiversityMode>2</DiversityMode>
            <HighGainLevel>-60</HighGainLevel>
            <LowGainLevel>-32</LowGainLevel>
            <GeneratorLevel>-60</GeneratorLevel>
            <VerificationLimit>1.8</VerificationLimit>
            <VerificationLimitGS>1.8</VerificationLimitGS>
            <CWOffsetFreq>67708</CWOffsetFreq>
            <VerificationCWFreqOffset>60000</VerificationCWFreqOffset>
            <CWOffsetRxlevQ4>0</CWOffsetRxlevQ4>
            <SUE2MinCallLimit>-12</SUE2MinCallLimit>
            <SUE2MaxCallLimit>5</SUE2MaxCallLimit>
            <AllowedMeasurementErrorQ4>160</AllowedMeasurementErrorQ4>
            <VerificationAverage>96</VerificationAverage>
            <sfcwidth>0.020</sfcwidth>
            <sfcfcbReps>15</sfcfcbReps>
        <MaxGainOffset>3</MaxGainOffset>
        </Rx>
        <Tx>
            <VerifyNumberPCLs>1</VerifyNumberPCLs>
            <PCLVerifLimit>
                <!-- SuperBand_0 is low bands -->
                <SuperBand_0>
                    <!-- Limitx will be used till PCLThresholdx -->
                    <Limit0>1.0</Limit0>
                    <Limit1>2</Limit1>
                    <!-- Threshold for PCL limit change -->
                    <PCLThreshold0>5</PCLThreshold0>
                </SuperBand_0>
                <!-- SuperBand_1 is high bands -->
                <SuperBand_1>
                    <!-- Limitx will be used till PCLThresholdx -->
                    <Limit0>1.0</Limit0>
                    <Limit1>2</Limit1>
                    <!-- Threshold for PCL limit change -->
                    <PCLThreshold0>0</PCLThreshold0>
                </SuperBand_1>
            </PCLVerifLimit>
            <Droop_correction>0.0</Droop_correction>
            <Channel>
                <Ch_Index_0>-1</Ch_Index_0>
                <Ch_Index_1>0</Ch_Index_1>
                <Ch_Index_2>4</Ch_Index_2>
                <Ch_Index_3>-1</Ch_Index_3>
            </Channel>
            <Channels>
                <Channel_Index_0_0>0</Channel_Index_0_0>
                <Channel_Index_0_1>1</Channel_Index_0_1>
                <Channel_Index_0_2>2</Channel_Index_0_2>
                <Channel_Index_0_3>3</Channel_Index_0_3>
                <Channel_Index_0_4>4</Channel_Index_0_4>
                <Channel_Index_1_0>0</Channel_Index_1_0>
                <Channel_Index_1_1>1</Channel_Index_1_1>
                <Channel_Index_1_2>2</Channel_Index_1_2>
                <Channel_Index_1_3>3</Channel_Index_1_3>
                <Channel_Index_1_4>4</Channel_Index_1_4>
                <Channel_Index_2_0>0</Channel_Index_2_0>
                <Channel_Index_2_1>1</Channel_Index_2_1>
                <Channel_Index_2_2>2</Channel_Index_2_2>
                <Channel_Index_2_3>3</Channel_Index_2_3>
                <Channel_Index_2_4>4</Channel_Index_2_4>
                <Channel_Index_3_0>0</Channel_Index_3_0>
                <Channel_Index_3_1>1</Channel_Index_3_1>
                <Channel_Index_3_2>2</Channel_Index_3_2>
                <Channel_Index_3_3>3</Channel_Index_3_3>
                <Channel_Index_3_4>4</Channel_Index_3_4>
            </Channels>
        </Tx>
        <BatteryAdj>
            <VBAT1>3.3</VBAT1>
            <VBAT2>4.2</VBAT2>
            <Limits>
                <VBATGainLimit>
                    <Low>1000</Low>
                    <Mid>8000</Mid>
                    <High>9999</High>
                </VBATGainLimit>
                <VBATOffsetLimit>
                    <Low>-1000</Low>
                    <Mid>0</Mid>
                    <High>1000</High>
                </VBATOffsetLimit>
                <VBAT_test>3.8</VBAT_test>
            </Limits>
            <VerificationLimit>
                <VBat_Max_error_mV>
                    <Low>-500</Low>
                    <Mid>0</Mid>
                    <High>500</High>
                </VBat_Max_error_mV>
            </VerificationLimit>
            <CustTag>0</CustTag>
        </BatteryAdj>
        <AFCAdjust>
            <VCXOadjustDACdelta>50</VCXOadjustDACdelta>
            <Adjustedfreqerrorlimit>
                <Low>-50</Low>
                <Mid>0</Mid>
                <High>50</High>
            </Adjustedfreqerrorlimit>
            <TemperatureLimits>
                <Low>15</Low>
                <High>45</High>
            </TemperatureLimits>
            <Tune>
                <Low>500</Low>
                <Mid>1500</Mid>
                <High>3596</High>
            </Tune>
            <Slope>
                <Low>3</Low>
                <Mid>7</Mid>
                <High>30</High>
            </Slope>
            <AFCAdjARFCNPCL>
                <GSM900>
                    <ChannelNumber>37</ChannelNumber>
                    <PCL>10</PCL>
                    <MaxPowOffset>10</MaxPowOffset>
                </GSM900>
                <GSM1800>
                    <ChannelNumber>698</ChannelNumber>
                    <PCL>5</PCL>
                    <MaxPowOffset>10</MaxPowOffset>
                </GSM1800>
                <GSM850>
                    <ChannelNumber>190</ChannelNumber>
                    <PCL>10</PCL>
                    <MaxPowOffset>10</MaxPowOffset>
                </GSM850>
                <GSM1900>
                    <ChannelNumber>661</ChannelNumber>
                    <PCL>5</PCL>
                    <MaxPowOffset>10</MaxPowOffset>
                </GSM1900>
            </AFCAdjARFCNPCL>
        </AFCAdjust>
    </GSM>
    <UMTS>
        <Common>
            <Nof_Umts_Bands>5</Nof_Umts_Bands>
            <Band_A>1</Band_A>
            <Band_B>2</Band_B>
            <Band_C>4</Band_C>
            <Band_D>5</Band_D>
            <Band_E>8</Band_E>
            <fspeeddelay>200</fspeeddelay>
