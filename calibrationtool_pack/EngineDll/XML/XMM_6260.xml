        <TX>
            <Min_OL_Power>-57</Min_OL_Power>
            <Limit_OL_Freq_Comp>80</Limit_OL_Freq_Comp>
            <Limit_CL_Freq_Comp>64</Limit_CL_Freq_Comp>
            <SUE2_InitCFG_Threshold>23500</SUE2_InitCFG_Threshold>
            <VerificationLimit>1.0</VerificationLimit>
            <RelaxedVerificationLimit>2.0</RelaxedVerificationLimit>
            <VerificationLimitMax>1.5</VerificationLimitMax>
            <VerificationLimitLowLevels>1.8</VerificationLimitLowLevels>
            <VerificationLimitLowLevelsThreshold>5</VerificationLimitLowLevelsThreshold>
            <UE2_OL_POWER_TOL>10</UE2_OL_POWER_TOL>
            <UE2_OL_POWER_MAX>6</UE2_OL_POWER_MAX>
            <UE2_CL_POWER_MIN>-6</UE2_CL_POWER_MIN>
            <UE2_CL_POWER_MAX>25</UE2_CL_POWER_MAX>
            <UE2_CL_Channel_comp_backof>6</UE2_CL_Channel_comp_backof>
            <UE2_PCTRL_Interpolation>1</UE2_PCTRL_Interpolation>
            <flip_UE2_OL_table>0</flip_UE2_OL_table>
