            <Nof_Umts_Bands>4</Nof_Umts_Bands>
            <Band_A>1</Band_A>
            <Band_B>2</Band_B>
            <Band_C>5</Band_C>
            <Band_D>8</Band_D>
        </Common>
        <Fast>
            <Method>1</Method>
        </Fast>
        <!---0 = Rx, 1 = Rd, 2 = Rx and Rd -->
        <RX>
            <RX3GDiversityMode>2</RX3GDiversityMode>
        </RX>
        <TX>
            <EnableEnvelopeTrackingCalibration>0</EnableEnvelopeTrackingCalibration>
        </TX>
